    <!-- Permission short label -->
    <string name="permlab_write_settings">تغییر تنظیمات و میانبُرهای خانه</string>
    <!-- Permission description -->
    <string name="permdesc_write_settings">به برنامه اجازهٔ تغییر تنظیمات و میانبُرهای خانه داده شود.</string>

